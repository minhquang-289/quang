import math
a,b,c = map(float,input("Hay nhap vao ba so bat ky:").split())
if a < b + c and b < a + c and c < a + b:
    p = (a + b + c)/2
    s = math.sqrt(p * (p-a) * (p-b) * (p-c))
    print("Dien tich cua tam giac la:",round(s,1))
else:
    print("Khong phai 3 canh cua tam giac")


